extends ../layout.pug

block head
    title='Bus Timings for ' + busStopName

block css
    link(rel='stylesheet' href='/static/css/timings.css')

block scripts
    script(src='/static/scripts/bus-timings.js')
    script(src='/static/scripts/idb.js')

block header
    img#page-icon(src='/static/images/bus-stop.svg')
    img#bookmark-status(src='/static/images/bookmark-empty.svg')
    span#title=busStopCode + ' ' + busStopName
block content
    include ../templates/bus-timings
